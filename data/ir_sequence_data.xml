<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="seq_isy_internal_stock_transfer" model="ir.sequence">
            <field name="name">ISY Internal Stock Transfer</field>
            <field name="code">isy.internal.stock.transfer</field>
            <field name="prefix">IST/%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_hr_payslip_approval" model="ir.sequence">
            <field name="name">ISY Payslip Approval</field>
            <field name="code">isy.hr.payslip.approval</field>
            <field name="prefix">HPA/%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>


        <record id="seq_hr_payslip_process_request" model="ir.sequence">
            <field name="name">ISY Payslip Process Request</field>
            <field name="code">hr.payslip.process.request</field>
            <field name="prefix">HPPR/%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_stock_audit_closing" model="ir.sequence">
            <field name="name">ISY Stock Audit Closing</field>
            <field name="code">stock.auditing</field>
            <field name="prefix">SKAG/%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>